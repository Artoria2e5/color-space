  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/color-space/issues"
  },
  "homepage": "https://github.com/dfcreative/color-space",
  "dependencies": {
    "color-parse": "^1.1.0",
    "color-space": "^1.1.3",
    "color-stringify": "^1.1.4",
    "color2": "^1.0.6",
    "emmy": "^5.2.8",
    "mumath": "^1.0.2",
    "queried": "^1.4.1"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "closurecompiler": "^1.5.2",
    "mocha": "^2.2.5",
    "watchify": "^3.2.2"
  }
}
