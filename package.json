{
  "name": "color-space-demo",
  "version": "1.0.0",
  "description": "Color-space demo",
  "main": "index.js",
  "scripts": {
    "build": "browserify -r emmy/on -r color-parse -r color-stringify -r color-space -r queried -r mumath | ccjs - --language_in=ECMASCRIPT5 > bundle.js",
    "watch": "watchify -r emmy/on -r color-parse -r color-stringify -r color-space -r queried -r mumath -d -o bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcreative/color-space.git"
  },
  "keywords": [
    "color",
    "space"
  ],
